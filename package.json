{
  "name": "slp-db",
  "version": "0.0.1",
  "main": "index.js",
  "author": "Mitch Dzugan <mitchdzugan@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "packager": "node node_modules/nwjs-packager/bin/index.js",
    "build__": "yarn run packager .",
    "build": "node scripts/build.js",
    "start": "nw ."
  },
  "dependencies": {
    "mutative": "^1.0.3",
    "preact": "^10.19.6",
    "realm": "^12.3.1",
    "xdg-app-paths": "^8.3.0"
  },
  "devDependencies": {
    "nw-builder": "^4.6.3",
    "nwjs-packager": "^1.5.1",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.0",
    "process": "^0.11.10",
    "url": "^0.11.0",
    "util": "^0.12.3"
  },
  "nwjs-packager": {
    "nwVersion": "latest",
    "appFriendlyName": "slp-db",
    "appMacIcon": "bin/icon.icns",
    "appWinIcon": "bin/icon.ico",
    "files": [
      "index.html, index.js, lib/**.*, node_modules/**/*"
    ],
    "builds": {
      "linux": {
        "tar.gz": true
      },
      "osx": {
        "zip": true
      },
      "win": {
        "zip": true
      }
    }
  },
  "targets": {
    "main": {
      "context": "node",
      "isLibrary": true,
      "sourceMap": false,
      "outputFormat": "commonjs",
      "includeNodeModules": {
        "typescript": false,
        "vscode": false
      }
    }
  }
}
